package kr.ac.kopo.vo;

public class SpamVO {
	private int spamCd;
	private String spamWord;
	private int memberCd;
	private String spamRegdate;
	
	public SpamVO() {
		
	}

	public SpamVO(int spamCd, String spamWord, String spamRegdate) {
		super();
		this.spamCd = spamCd;
		this.spamWord = spamWord;
	}

	public int getSpamCd() {
		return spamCd;
	}

	public void setSpamCd(int spamCd) {
		this.spamCd = spamCd;
	}

	public String getSpamWord() {
		return spamWord;
	}

	public void setSpamWord(String spamWord) {
		this.spamWord = spamWord;
	}

	public int getMemberCd() {
		return memberCd;
	}

	public void setMemberCd(int memberCd) {
		this.memberCd = memberCd;
	}

	public String getSpamRegdate() {
		return spamRegdate;
	}

	public void setSpamRegdate(String spamRegdate) {
		this.spamRegdate = spamRegdate;
	}
	
	
	
	
}
