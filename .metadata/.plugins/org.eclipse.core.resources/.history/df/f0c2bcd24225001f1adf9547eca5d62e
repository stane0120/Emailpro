package kr.ac.kopo.vo;

public class MailVO {
	private int mailCd;
	private String mailTitle;
	private String mailContent;
	private int mailSenderCd;
	private int mailReceiverCd;
	private int mailOpenChk;
	private String mailSentDate;

	public MailVO() {
		super();
	}

	
	
	public MailVO(String mailTitle, String mailContent, int mailSenderCd, int mailReceiverCd) {
		super();
		this.mailTitle = mailTitle;
		this.mailContent = mailContent;
		this.mailSenderCd = mailSenderCd;
		this.mailReceiverCd = mailReceiverCd;
	}

	

	public MailVO(int mailCd, String mailTitle, String mailContent, int mailSenderCd, int mailReceiverCd, String mailSentDate) {
		super();
		this.mailCd = mailCd;
		this.mailTitle = mailTitle;
		this.mailContent = mailContent;
		this.mailSenderCd = mailSenderCd;
		this.mailReceiverCd = mailReceiverCd;
		this.mailSentDate = mailSentDate;
	}



	public int getMailCd() {
		return mailCd;
	}

	public void setMailCd(int mailCd) {
		this.mailCd = mailCd;
	}

	public String getMailTitle() {
		return mailTitle;
	}

	public void setMailTitle(String mailTitle) {
		this.mailTitle = mailTitle;
	}

	public String getMailContent() {
		return mailContent;
	}

	public void setMailContent(String mailContent) {
		this.mailContent = mailContent;
	}

	public int getMailSenderCd() {
		return mailSenderCd;
	}

	public void setMailSenderCd(int mailSenderCd) {
		this.mailSenderCd = mailSenderCd;
	}

	public int getMailReceiverCd() {
		return mailReceiverCd;
	}

	public void setMailReceiverCd(int mailReceiverCd) {
		this.mailReceiverCd = mailReceiverCd;
	}

	public int getMailOpenChk() {
		return mailOpenChk;
	}

	public void setMailOpenChk(int mailOpenChk) {
		this.mailOpenChk = mailOpenChk;
	}

	public String getMailSentDate() {
		return mailSentDate;
	}

	public void setMailSentDate(String mailSentDate) {
		this.mailSentDate = mailSentDate;
	}	
	

}
